<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.validation.examples.category"
            name="%Validation_category"
            parentCategory="org.eclipse.ui.Examples"/>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.eclipse.emf.validation.examples.category"
            class="org.eclipse.emf.validation.examples.internal.wizard.GeneralExampleWizard"
            hasPages="false"
            id="org.eclipse.emf.validation.examples.general"
            name="General Validation Example">
         <description>%Validation_General_description</description>
      </wizard>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.eclipse.emf.validation.examples.category"
            class="org.eclipse.emf.validation.examples.internal.wizard.AdapterExampleWizard"
            hasPages="false"
            id="org.eclipse.emf.validation.examples.adapter"
            name="Validation Adapter Example">
         <description>%Validation_Adapter_description</description>
      </wizard>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/org.eclipse.emf.validation.examples.category"
            class="org.eclipse.emf.validation.examples.internal.wizard.OCLExampleWizard"
            hasPages="false"
            id="org.eclipse.emf.validation.examples.ocl"
            name="OCL Example">
         <description>%Validation_OCL_description</description>
      </wizard>
   </extension>

</plugin>
